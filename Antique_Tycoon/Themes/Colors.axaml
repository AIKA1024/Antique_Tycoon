<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  
  <Color x:Key="BorderColor">#1E1E1F</Color>
  <Color x:Key="LightedBorderColor">#5A5B5C</Color>
  <Color x:Key="BorderBackgroundColor">#48494A</Color>
  <Color x:Key="BorderBottomColor">#323334</Color>
  <SolidColorBrush x:Key="BorderBrush" Color="{StaticResource BorderColor}" />
  <SolidColorBrush x:Key="LightedBorderBrush" Color="{StaticResource LightedBorderColor}" />
  <SolidColorBrush x:Key="BorderBackgroundBrush" Color="{StaticResource BorderBackgroundColor}" />
  <SolidColorBrush x:Key="BorderBottomBrush" Color="{StaticResource BorderBottomColor}" />
</ResourceDictionary>
